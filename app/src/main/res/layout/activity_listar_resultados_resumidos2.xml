<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".activity.Administrador.ExibirResultados.ListarResultadosResumidasActivity">

    <TextView
        android:id="@+id/tv_listarResultadosResumidos_nomeQuestionario"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:padding="8dp"
        android:text="Selecione um QuestionÃ¡rio da Lista"
        android:textColor="@color/vermelho"
        android:textSize="24sp" />

    <Button
        android:id="@+id/bt_listarResultadosResumidos_confirmar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_margin="8dp"
        android:onClick="clicouConfirmarQuestionario"
        android:padding="8dp"
        android:text="Dados Quantitativos" />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_margin="8dp"
        android:padding="8dp"
        android:text="Button" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recycler_view_mostrar_resultados_resumidos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="8dp"
        android:clickable="true"
        android:padding="8dp" />
</LinearLayout>